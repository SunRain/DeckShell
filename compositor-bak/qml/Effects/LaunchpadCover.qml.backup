import QtQuick

Rectangle {
    id: root

    anchors.fill: parent
    color: "black"
    opacity: 0.5

    signal clicked

    MouseArea {
        anchors.fill: parent
        onClicked: {
            root.clicked()
        }
    }

    Text {
        anchors.centerIn: parent
        text: "Click to close Launchpad"
        font.pointSize: 16
        color: "white"
        opacity: 0.8
    }
}